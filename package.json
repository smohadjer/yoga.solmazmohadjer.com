{
	"name": "solmazmohadjer",
	"version": "1.0.0",
	"description": "Yoga Courses",
	"repository": {
		"type": "git",
		"url": "https://github.com/smohadjer/yoga.solmazmohadjer.com"
	},
	"license": "MIT",
	"scripts": {
		"precompile": "node node_modules/build/bin/clean.js",
		"compile": "node node_modules/build/bin/compile.js",
		"copy": "node node_modules/build/bin/copyPublic.js",
		"copyDist": "node node_modules/build/bin/copydist.js",
		"sass": "node node_modules/build/bin/sassTocss",
		"hbs": "node node_modules/build/bin/hbs.js",
		"generateSchedules": "node app/bin/jsonToHtml.js",
		"fetchNextClass": "node app/bin/fetchNextClass.js",
		"watch": "node node_modules/build/bin/watch-content.js & node node_modules/build/bin/watch-css.js & node node_modules/build/bin/watch-js.js & node node_modules/build/bin/watch-img.js",
		"server": "node node_modules/build/bin/server.js",
		"serve": "npm run fetchNextClass && npm run generateSchedules && npm run compile && npm run copy && npm run sass && npm run hbs",
		"start": "npm run serve && npm run watch & npm run server",
		"build": "npm run serve && rollup -c && npm run copyDist",
		"build-api": "npm install && node app/bin/fetchDependencies.js"
	},
	"devDependencies": {
		"build": "git://github.com/smohadjer/build.git#0.1.11",
		"handlebars": "^4.7.7",
		"moment": "2.29.4",
		"express": "^4.16.2",
		"fs-extra": "^8.1.0"
	},
	"dependencies": {
		"hamburgers": "0.9.3",
		"helpers": "git+https://github.com/smohadjer/helpers.git",
		"youtube-playlist": "git://github.com/smohadjer/youtube-playlist.git",
		"filterList": "git+https://github.com/smohadjer/filterList#3.1.0"
	},
	"engines": {
		"node": ">=16.13.0"
	}
}
